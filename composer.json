    "keywords": ["templating", "mustache", "symfony", "sf2", "php"],
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-0": {
            "Propertyguru\\MustacheBundle": ""
        }
    },
    "target-dir": "Propertyguru/MustacheBundle",
